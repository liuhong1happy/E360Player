{
  "name": "@17ued/e360player",
  "version": "1.0.0",
  "description": "`E360Player` is the `360Â° Video Player ` with [electron](https://github.com/atom/electron).",
  "main": "app/main.js",
  "scripts": {
    "start": "electron ./app/main.js --inspect",
    "pack-osx": "electron-packager ./app E360Player --platform=darwin --arch=x64 --electron-version=5.0.1",
    "pack-win32": "electron-packager ./app E360Player --platform=win32 --arch=ia32 --electron-version=5.0.1",
    "pack-win64": "electron-packager ./app E360Player --platform=win32 --arch=x64 --electron-version=5.0.1",
    "pack-linux": "electron-packager ./app E360Player --platform=linux --arch=x64 --electron-version=5.0.1",
    "postinstall": "electron-builder install-app-deps",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "release": "build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liuhong1happy/E360Player.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/liuhong1happy/E360Player/issues"
  },
  "homepage": "https://github.com/liuhong1happy/E360Player#readme",
  "devDependencies": {
    "electron": "^5.0.1",
    "electron-builder": "^20.43.0",
    "electron-packager": "^13.1.1"
  },
  "build": {
    "appId": "com.17ued.e360player",
    "mac": {
      "category": "public.app-category.video-player",
      "target": "dmg",
      "asar": false
    },
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    }
  }
}
